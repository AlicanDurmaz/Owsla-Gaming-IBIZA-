		foreach(new playerid:Player)
		{
		    if(IsPlayerInDynamicArea(playerid, bolgedyzone[bolgid]) && (PlayerInfo[playerid][ceteid] == cetid || PlayerInfo[playerid][ceteid] == Bolge[bolgid][bolgesahip]))
			{
				RemovePlayerMapIcon( playerid, bolgid );
				TextDrawHideForPlayer(playerid,Zonebilgi[playerid]);
				zonebilgiacik[playerid] = 0;
