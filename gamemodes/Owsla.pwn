stock DriverCheck(vehicleid)
{
	for(new i = 0; i <= GetPlayerPoolSize(); i++)
	{
		if(IsPlayerConnected(i))
		{
			if(GetPlayerState(i) == PLAYER_STATE_DRIVER && GetPlayerVehicleID(i) == vehicleid) return 1;
		}
	}
	return 0;
}
stock GetVehicleSpeed(playerid)
{
	new Float:x, Float:y, Float:z;
	GetVehicleVelocity(GetPlayerVehicleID(playerid), x, y, z);
	return floatround(floatsqroot(x*x+y*y+z*z)*200.2);
}
public OnPlayerStateChange(playerid, newstate, oldstate)
{
	if(GetPlayerState(playerid) == 9 && Spectekiler[playerid] == false)
	{
	    KickReason(playerid,"Invisible Hack");
	    return 1;
	}
	if(GetPVarInt(playerid,"AntiFloodState") > GetTickCount())
	{
		if(stateflood{playerid} > 4)
		{
			KickReason(playerid,"Car Hack");
			return 1;
		}
		stateflood{playerid}++;
	}else stateflood{playerid} = 0;
	SetPVarInt(playerid, "AntiFloodState", GetTickCount()+400);

    if(oldstate == PLAYER_STATE_DRIVER) KillTimer(PlayerInfo[playerid][NosTimer]);
	if(newstate == PLAYER_STATE_DRIVER)
	{
		TextDrawShowForPlayer(playerid, KMH0[playerid]);
		TextDrawShowForPlayer(playerid, KMH1[playerid]);
		TextDrawShowForPlayer(playerid, KMH3[playerid]);
		TextDrawShowForPlayer(playerid, KMH5[playerid]);
		ShowPlayerProgressBar(playerid, SpeedoBar[playerid]);
	} else {
		TextDrawHideForPlayer(playerid, KMH0[playerid]);
		TextDrawHideForPlayer(playerid, KMH1[playerid]);
		TextDrawHideForPlayer(playerid, KMH3[playerid]);
		TextDrawHideForPlayer(playerid, KMH5[playerid]);
		HidePlayerProgressBar(playerid, SpeedoBar[playerid]);
	}
	if(newstate == PLAYER_STATE_DRIVER){
 	if(derbyde{playerid} == 0)SetPlayerArmedWeapon(playerid,0);
	}
	if(newstate == PLAYER_STATE_ONFOOT && (Joined{playerid} == true)){
	new Float:x,Float:y,Float:z,Float:a;
	GetPlayerPos(playerid,x,y,z);
	if(ivehicle[playerid] == INVALID_VEHICLE_ID || (GetVehicleModel(ivehicle[playerid]) != GetPVarInt(playerid,"RaceVehiclem")) && RaceStarted == 1){
	if(Joined{playerid} == true){
	DestroyVehicleEx(ivehicle[playerid]);
	GetPlayerFacingAngle(playerid,a);
	if(z <= -1)	PlayerRaceVehicle(playerid,CPCoords[CPProgess[playerid]-1][0],CPCoords[CPProgess[playerid]-1][1],CPCoords[CPProgess[playerid]-1][2],a);
	else PlayerRaceVehicle(playerid,x,y,z,a);
    return 1;
   	}
   	}else{
   	if(z<=0){
	if(CPProgess[playerid]-1>0){
	SetVehiclePos(ivehicle[playerid],CPCoords[CPProgess[playerid]-1][0], CPCoords[CPProgess[playerid]-1][1], CPCoords[CPProgess[playerid]-1][2]);
	}else{
	SetVehiclePos(ivehicle[playerid],RaceVehCoords[0][0], RaceVehCoords[0][1], RaceVehCoords[0][2]);
	}
	}
	PutPlayerInVehicle(playerid, ivehicle[playerid], 0);
	return 1;
	}
	}
	if(newstate == PLAYER_STATE_DRIVER || newstate == PLAYER_STATE_PASSENGER)
	{
		foreach(new i:Player)
    	{
    	    if(PlayerInfo[i][SpecID] == playerid)StartSpectate(i,playerid);
    	}
		if(Joined{playerid} == true || derbyde{playerid} == 1 || shootde{playerid} == 1)return 1;
    }
    if(oldstate == PLAYER_STATE_DRIVER)
	{
		foreach(new i:Player)
    	{
    	    if(PlayerInfo[i][SpecID] == playerid)StartSpectate(i,playerid);
    	}
		for(new i=0;i<MAX_PLAYERS;i++)
		{
			SetVehicleParamsForPlayer(GetPlayerVehicleID(playerid),i, 0, 0);
		}
	}
	return 1;
}
