stock akacheck(playerid)
{
	new Query[2048], serial[72], names[25], count = 1;
    gpci(playerid,serial,72);
    mysql_format(Owsla, Query, sizeof(Query), "SELECT * FROM `sban` WHERE `serial` = '%e' AND `time` > '%d'", serial, gettime());
	new Cache:cache = mysql_query(Owsla, Query);
	if(cache_num_rows())
	{
		BanReason(playerid, "SBan");
	}
	cache_delete(cache);
    mysql_format(Owsla, Query, sizeof(Query), "SELECT * FROM `akas` WHERE `serial` = '%e' AND `name` = '%e'", serial,PlayerName(playerid));
	cache = mysql_query(Owsla, Query);
	if(!cache_num_rows())
	{
		mysql_format(Owsla, Query, sizeof(Query), "INSERT INTO `akas` (`serial`, `name`) VALUES ('%e', '%e')", serial, PlayerName(playerid));
    	mysql_tquery(Owsla, Query);
	}
	cache_delete(cache);
	mysql_format(Owsla, Query, sizeof(Query), "SELECT * FROM `akas` WHERE `serial` = '%e'", serial);
	cache = mysql_query(Owsla, Query);
	new rows = cache_num_rows();
	if(rows > 0)
	{
		new str[1024];
		for(new i; i < rows; i++)
		{
			cache_get_value_name(i, "name", names);
			format(str, 2048, "%s{93FF93}%d){93FF93}\t\t%s\n", str, count, names);
			count++;
 		}
        SetPVarString(playerid, "akas", str);
	}
	cache_delete(cache);
}
