function YarisHesapla()
{
	new
	Float:fx,
    Float:fy,
	Float:fz,
	Float:uzaklik[MAX_PLAYERS];
	foreach(new i:racer)
	{
	    GetPlayerPos(i,fx,fy,fz);
	    fx -= CPCoords[CPProgess[i]][0];
	    fy -= CPCoords[CPProgess[i]][1];
	    fz -= CPCoords[CPProgess[i]][2];
	    uzaklik[i] = ((fx * fx) + (fy * fy) + (fz * fz));
	    RacePosition[i] = 1;
 	}
	foreach(new playerid:racer)
	{
		foreach(new i:racer)
		{
			if(i != playerid)
			{
	    		if(CPProgess[playerid] < CPProgess[i])RacePosition[playerid]++;
	    		else if(CPProgess[playerid] == CPProgess[i])
				{
	    			if(uzaklik[playerid] > uzaklik[i])RacePosition[playerid]++;
	    		}
			}
			RacePosition[playerid] += FinishCount;
			TextInfo(playerid);
		}
	}
	return 1;
}
function PickupCheck()
{
	new
	Float:pos[4],
	Float:Vhiz[3],
	Float:can;
	foreach(new i:racer)
	{
		GetPlayerPos(i,pos[0],pos[1],pos[2]);
    	foreach(new c:nosp)
	    {
		    if(IsPlayerInRangeOfPoint(i, 3.5, mPickInfo[c][sX], mPickInfo[c][sY], mPickInfo[c][sZz]))
		    {
		    	if(IsPlayerInAnyVehicle(i))
				{
					if(GetTickCount() - tNitro[i] > 3000)
					{
						tNitro[i] = GetTickCount();
		    			AddVehicleComponent(GetPlayerVehicleID(i), 1010);
		    			PlayerPlaySound(i, 1133 ,0, 0, 0);
		    		}
				}
		    }
	    }
   		foreach(new c:rep)
	    {
		    if(IsPlayerInRangeOfPoint(i, 3.5, mPickInfo[c][sX], mPickInfo[c][sY], mPickInfo[c][sZz]))
		    {
		        if(IsPlayerInAnyVehicle(i))
				{
					if(GetTickCount() - tRepair[i] > 3000)
					{
						tRepair[i] = GetTickCount();
		    			RepairVehicle(GetPlayerVehicleID(i));
		    			PlayerPlaySound(i, 1133 ,0, 0, 0);
		    		}
				}
		    }
	    }
	    foreach(new c:vehp)
	    {
		    if(IsPlayerInRangeOfPoint(i, 3.5, mPickInfo[c][sX], mPickInfo[c][sY], mPickInfo[c][sZz]))
		    {
		        new vehid = GetPlayerVehicleID(i);
		    	if(GetVehicleModel(vehid) != vehchcar[c]){
			    GetVehiclePos(vehid,pos[0],pos[1],pos[2]);
			    GetVehicleZAngle(vehid,pos[3]);
			    GetVehicleVelocity(vehid,Vhiz[0],Vhiz[1],Vhiz[2]);
			    GetVehicleHealth(vehid,can);
			    DestroyVehicleEx(ivehicle[i]);
			    ivehicle[i] = CreateVehicle(vehchcar[c],pos[0],pos[1],pos[2]+1,pos[3],-1,-1,-1);
			    for(new as; as < 5; as++)
				{
					if(VehicleSave[as][i][17] == vehchcar[c])
					{
						for(new a; a < 14;a++)
						{
							if(VehicleSave[as][i][a] != 0)AddVehicleComponent(ivehicle[i], VehicleSave[as][i][a]);
						}
						if(VehicleSave[as][i][14] != 3)ChangeVehiclePaintjob(ivehicle[i], VehicleSave[as][i][14]);
						ChangeVehicleColor(ivehicle[i], VehicleSave[as][i][15], VehicleSave[as][i][16]);
						break;
					}
				}
			    SetPVarInt(i,"RaceVehiclem",vehchcar[c]);
			    SetVehicleVirtualWorld(ivehicle[i],10);
			    PutPlayerInVehicle(i,ivehicle[i],0);
			    s_AirbreakUpdateTick[i] = gettime() + 3;
			    SetVehicleVelocity(ivehicle[i],Vhiz[0],Vhiz[1],Vhiz[2]);
			    SetVehicleHealth(ivehicle[i],can);
			    return 1;
	   			}
		    }
	    }
	}
	return 1;
}
