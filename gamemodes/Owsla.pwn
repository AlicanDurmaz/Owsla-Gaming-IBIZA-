stock strcount(const string[], const sub[], bool:ignorecase = false, bool:count_overlapped = false)
{
	new
		increment = count_overlapped ? 1 : strlen(sub),
		pos = -increment,
		count = 0
	;
	while (-1 != (pos = strfind(string, sub, ignorecase, pos + increment)))
		count++;
	return count;
}
