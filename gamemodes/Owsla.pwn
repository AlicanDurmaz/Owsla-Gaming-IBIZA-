function MinigameCountdown()
{
	if(Iter_Count(_Minigamer) < 2)
	{
		TextMesaji("~y~~h~[FALLOUT] ~w~~h~Fallout iptal edildi! (~r~~h~Yetersiz katilim~w~~h~)");
		foreach(new i: _Minigamer) Minigamer_{ i } = false,SpawnPlayerEx(i);
		return EndMinigame();
	}
	if(inProgress != 2)
	{
  		foreach(new i: _Minigamer)
		{
			ResetPlayerWeapons( i );
			SetPlayerInterior( i, 0 );
     		SetPlayerCameraPos( i, -5298.4814,-218.4391,42.1386);
     		SetPlayerCameraLookAt( i, -5298.1616,-189.6903,23.6564);
     		TogglePlayerControllable( i, false );
     		GameTextForPlayer(i,"~g~~h~~h~Oyun Basliyor",2000,3);
		}
		falloutkatilimbitti = true;
		Iter_Clear(_Objects);
		for(new i; i < MAX_SLOTS; i++) Iter_Add(_Objects, i);
		SetTimer("MinigameCountdown", 2000, 0);
		inProgress = 2;
	}
	else
	{
		foreach(new i: _Minigamer)
		{
		    if(!VIEW_FROM_ABOVE)
			SetCameraBehindPlayer( i );
			PlayerPlaySound( i, 1057, 0.0, 0.0, 0.0 );
			TogglePlayerControllable( i, true );
			GameTextForPlayer(i,"~g~~h~~h~Oyun Basladi",2000,3);
		}
		uTimer = SetTimer( "MinigameUpdate", 2500, 1 );
	}
	return 1;
}

function MinigameUpdate()
{
	if( Iter_Count(_Minigamer) < 1 ) return EndMinigame();
	new str[128], Float:playerx, Float:playery, Float:playerz;
	foreach(new i: _Minigamer)
	{
	    if(i > MAX_PLAYERS)continue;
		GetPlayerPos( i, playerx, playery, playerz );
		if(playerz < 2.0)
		{
			format(str, sizeof(str), "~r~~h~%s ~w~~h~dustu!", PlayerName(i));
			foreach(new a: _Minigamer)GameTextForPlayer(a, str ,5000,3);
