function ParkourCheck()
{
	if(OnlineParkour() == 0) return 1;
	if(ParkourInfo[Durum] == true) return 1;
	new string[156], bool: parkourlutfen;
    parkourlutfen = false;
	foreach(new x: Player)
	{
	    if(PlayerInfo[x][Parkour] == true)
	    {
			if(IsPlayerWater(x))
			{
			    OnPlayerSpawn(x);
			    parkourlutfen = true;
			    format(string, sizeof string, "~w~~h~%s ~r~~h~elendi!", PlayerName(x));
			}
		}
	}
	if(parkourlutfen == true)
	{
		foreach(new ba: Player)
		{
			if(PlayerInfo[ba][Parkour] == true)
			{
				GameTextForPlayer(ba, string, 5000, 6);
			}
		}
	}
	return 1;
}

