dcmd_makeadmin(playerid,params[])
{
	if(PlayerInfo[playerid][LoggedIn] != 1) return SendClientMessageEx(playerid,0xFF0000FF,"Hata » {FFFFFF}Bu komutu kullanmak icin sunucuya giris yapmis olmalisiniz.");
 	if(!IsPlayerAdmin(playerid)) return YETERSIZSEVIYE(playerid,10);
	new tmp[256], tmp2[256], Index;
	tmp = strtok(params,Index), tmp2 = strtok(params,Index);
 	if(!strlen(params)) return SendClientMessageEx(playerid, 0x66FFFFFF,"Bilgi » {FFFFFF}/makeadmin [playerid] [level]");
 	new player1, level, string[256];
	player1 = strval(tmp);
	if(!strlen(tmp2)) return SendClientMessageEx(playerid, 0x66FFFFFF,"Bilgi » {FFFFFF}/makeadmin [playerid] [level]");
	level = strval(tmp2);
	if(!IsPlayerConnected(player1)) return SendClientMessageEx(playerid,0xFF0000FF,"Hata » {FFFFFF}Oyuncu oyunda deðil.");
	if(PlayerInfo[player1][LoggedIn] == 1)
	{
		if(level > 10 ) return SendClientMessageEx(playerid,0xFF0000FF,"Hata » {FFFFFF}Gecersiz Level Lutfen 1 ile 10 arasinda bir sayi kullaniniz.");
		if(level == PlayerInfo[player1][Level]) return SendClientMessageEx(playerid,0xFF0000FF,"Hata » {FFFFFF}Bu oyuncu zaten admin.");
		new year,month,day;   getdate(year, month, day);
		new hour,minute,second; gettime(hour,minute,second);
		format(string,sizeof(string),"Admin »{ffffff} %s isimli yonetici admin seni - %d - level admin yapti.",PlayerName(playerid), level);
		SendClientMessageEx(player1,0xCC9900FF,string);
		if(level > PlayerInfo[player1][Level])
		{
			GameTextForPlayer(player1,"~b~~h~~h~Admin oldunuz tebrikler.", 2000, 3);
		}else
		{
			GameTextForPlayer(player1,"~b~~h~~h~Admin leveliniz degisti. Kontrol ediniz.", 2000, 3);
		}
		format(string,sizeof(string),"Admin »{ffffff} %s isimli oyuncunun Levelini %d yaptin. Tarih  %d/%d/%d, Zaman %d:%d:%d", PlayerName(player1), level, day, month, year, hour, minute, second); SendClientMessageEx(playerid,0xCC9900FF,string);
		format(string,sizeof(string),"Admin »{ffffff} %s isimli admin %s isimli oyuncuyu %d level admin tyapti. Tarih %d/%d/%d Zaman %d:%d:%d",PlayerName(playerid), PlayerName(player1), level, day, month, year, hour, minute, second);
		PlayerInfo[player1][Level] = level;
		PlayerInfo[player1][Animator] = 0;
		PlayerInfo[player1][DJ] = 0;
		labelguncelle(player1);
		SavePlayer(player1);
		return PlayerPlaySound(player1,1057,0.0,0.0,0.0);
	} else return SendClientMessageEx(playerid,0xFF0000FF,"Hata » {FFFFFF}Oyuncu sunucuya kayitli degil veya giris yapmamis.");
}

