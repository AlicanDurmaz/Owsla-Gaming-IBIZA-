	if(sscanf(params,"uds[20]s[20]",player1,skor,tmp1,tmp2))return SendClientMessageEx(playerid,0x66FFFFFF,"Bilgi » {FFFFFF}/duello [playerid] [skor] [1. silahid/silahismi] [2. silahid/silahismi]");
 	if(!IsPlayerConnected(player1))return SendClientMessageEx(playerid,0xFF0000FF,"Hata » {FFFFFF}Oyuncu oyunda deðil.");
	if(duelloengelli[player1] == true)return SendClientMessageEx(playerid,0x66FFFFFF,"Bilgi » {FFFFFF}Bu oyuncunun düello isteði kapalýdýr. Sizde kapatmak için {0077FF}/duellokapat {FFFFFF}kullanýn.");
	if(duelloda[player1] == 1)return SendClientMessageEx(playerid,0x66FFFFFF,"Bilgi » {FFFFFF}Bu oyuncu zaten düelloda.");
	if(IsPlayerPaused(player1)) return SendClientMessageEx(playerid, 0x66FFFFFF, "Bilgi » {FFFFFF}Oyuncu AFK.");
	if(!IsNumeric(tmp1))silahid1 = GetWeaponIDFromName(tmp1); else silahid1 = strval(tmp1);
	if(!IsNumeric(tmp2))silahid2 = GetWeaponIDFromName(tmp2); else silahid2 = strval(tmp2);
	if(!IsValidWeapon(silahid1) || silahid1 == 0 || !IsValidWeapon(silahid2) || silahid2 == 0)return SendClientMessageEx(playerid,0x66FFFFFF,"Bilgi » {FFFFFF}Geçersiz silah id.");
	if(GetPlayerScore(player1) < skor)return SendClientMessageEx(playerid,0x66FFFFFF,"Bilgi » {FFFFFF}Rakibinin Skoru Yetersiz.");
	if(GetPlayerScore(playerid) < skor)return SendClientMessageEx(playerid,0x66FFFFFF,"Bilgi » {FFFFFF}Rakibinin Skoru Yetersiz.");
	if(skor > 50 || skor < 0)return SendClientMessageEx(playerid,0x66FFFFFF,"Bilgi » {FFFFFF}Skor 0 - 50 arasýnda olmalýdýr.");
	if(SureYasagi(playerid, "duel", 20)) return 1;
	GetWeaponName(silahid1,WeapName,32);
	GetWeaponName(silahid2,WeapName2,32);
	format(estring,512,"{93FF93}%s {FFFFFF}Ýsimli oyuncu sizi düelloya davet etti.\n\nDüello Silahlarý:\n\t{FFFFFF}Silah-1: {93FF93}%s\n\t{FFFFFF}Silah-2: {93FF93}%s\n{FFFFFF}Düello Bahis skoru = {93FF93}%d",PlayerName(playerid),WeapName,WeapName2,skor);
	ShowPlayerDialog(player1,1387,DIALOG_STYLE_MSGBOX,"{93FF93}Owsla Gaming - {FFFFFF}Düello Daveti",estring,"Kabul","Red");
	format(estring,512,"Bilgi » {FFFFFF}%s Ýsimli oyuncuyu düelloya davet ettiniz.",PlayerName(player1));
	SendClientMessageEx(playerid, 0x66FFFFFF, estring);
	dellodavetci[player1] = playerid;
	dellodavetci[playerid] = player1;
	duelloskor[player1] = skor;
	duelloskor[playerid] = skor;
	duellosilah[player1] = silahid1;
	duellosilah[playerid] = silahid1;
 	duellosilah2[player1] = silahid2;
	duellosilah2[playerid] = silahid2;
	return 1;
}

