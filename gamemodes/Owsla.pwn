function SolarFall()
{
	new objectid, go;
	new alive;
	for(new i = 0; i < 100; i++)
	{
		if(Info[numberout][i] == -1) go++;
	}
	foreach(new a:Player)
	{
		if(faloutta{a} == 1)alive++;
	}
	if(go == 3)
	{
		if(Info[dead] == 0) SetTimer("DecideWinners", 100, 0);
        if(Info[timer][0]!=-1)KillTimer(Info[timer][0]),Info[timer][0]=-1;
		Info[dead] = 1;
		return 1;
	}
	if(alive < 2 && Info[dead] == 0)
	{
		SetTimer("DecideWinners", 100, 0);
		if(Info[timer][0]!=-1)KillTimer(Info[timer][0]),Info[timer][0]=-1;
		Info[dead] = 1;
		return 1;
	}
	start:
	objectid = random(100);
	if(Info[numberout][objectid] != -1)
	{
		#emit JUMP start
	}
	Info[numberout][objectid] = 0;
	Info[shaketimer][objectid] = SetTimerEx("SquareShake", 100, true, "ii", falloutob[objectid],objectid);
	return 1;
}

function CountDown()
{
	if(falltime > GetTickCount())return 1;
	falltime = GetTickCount()+500;
	new string[128], number[8], oyuncu;
	string = "~g~~h~ Basliyor ~y~~h~";
	format(number, sizeof(number), "%d", Info[counts]);
	strins(string, number, strlen(string));
	foreach(new i: Player)
	{
		if(faloutta{i} == 1)
		{
			GameTextForPlayer(i,string, 1000, 3);
			oyuncu++;
		}
	}
	if(Info[counts] <= 0)
	{
		if(Info[timer][1]!=-1)KillTimer(Info[timer][1]),Info[timer][1]=-1;
		falloutkatilimbitti = true;
		if(oyuncu < 2)
		{
			falloutiptal();
			return 1;
		}
		StartFalling();
		return 1;
	}
	SetTimer("CountDown", 1000, 0);
	return Info[counts]--;
}
