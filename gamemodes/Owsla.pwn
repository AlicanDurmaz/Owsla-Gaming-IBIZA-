stock unIP(const mStr[])
{
	new	sayi, warn, i, sayiarasi, teksayi;
	while(mStr[i] != EOS)
	{
		if('0' <= mStr[i] <= '9')
		{
			if(warn >= 3)return 1;
			if(sayiarasi > 2 && warn > 1)
			{
				warn--;
			}
			sayiarasi=0;
			sayi++;
			if(sayi == 2 || sayi == 3)
			{
				if((mStr[i+1] == '.' || mStr[i+1] == ',' || mStr[i+1] == '/' || mStr[i+1] == ' ') && '0' <= mStr[i+2] <= '9')
				{
					warn++;
					sayi=0;
				}
			}else
			if(sayi == 1)
			{
			    if((mStr[i+1] == '.' || mStr[i+1] == ',' || mStr[i+1] == '/' || mStr[i+1] == ' ') && '0' <= mStr[i+2] <= '9')
				{
				    if(teksayi < 1)warn++;
					sayi=0;
					teksayi++;
				}
			}
		}else
		{
			sayiarasi++;
			sayi=0;
		}
		i++;
	}
	return 0;
}
