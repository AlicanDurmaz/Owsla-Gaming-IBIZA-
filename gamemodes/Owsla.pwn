function SquareShake(objectid, d)
{
	if(d == falloutob[0])
	{
		if(Info[shaketimer][d]!=-1)KillTimer(Info[shaketimer][d]),Info[shaketimer][d]=-1;
		return 1;
	}
	switch(Info[shake][d])
	{
		case 0, 5: SetObjectRot(objectid, 31.8, 2, 0);
		case 1, 6: SetObjectRot(objectid, 33.8, 0, 0);
		case 2, 7: SetObjectRot(objectid, 31.8, -2, 0);
		case 3, 8: SetObjectRot(objectid, 29.8, 0, 0);
		case 4, 9: SetObjectRot(objectid, 31.8, 0, 0);
		case 10:
		{
			new Float:X, Float:Y, Float:Z;
			GetObjectPos(objectid, X, Y, Z);
			MoveObject(objectid, X, Y, Z - 100, 40);
		}
		case 11..99: SetObjectRot(objectid, 31.8 - ((Info[shake][d] * 2) - 20), 0, 0);
		case 100:
		{
			DestroyObject(objectid);
   			if(Info[shaketimer][d]!=-1)KillTimer(Info[shaketimer][d]),Info[shaketimer][d]=-1;
		}
	}
	Info[shake][d]++;
	return 1;
}

