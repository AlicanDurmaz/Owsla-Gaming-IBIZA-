function IslemUygula(playerid, action)
{
	switch(action)
	{
 		case 1: Kick(playerid);
     	case 2:
		{
			new str[256];
			mysql_format(Owsla, str, 256, "INSERT INTO `blockip` (`ip`,`player`) VALUES ('%s','%s')",getIp(playerid), PlayerName(playerid));
