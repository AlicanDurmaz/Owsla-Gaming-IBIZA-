    		foreach(new playerid:Player)
			{
			    if(IsPlayerInDynamicArea(playerid, bolgedyzone[bolgid]) && (PlayerInfo[playerid][ceteid] == cetid || PlayerInfo[playerid][ceteid] == Bolge[bolgid][bolgesahip]))
				{
					RemovePlayerMapIcon( playerid, bolgid );
					TextDrawHideForPlayer(playerid,Zonebilgi[playerid]);
					zonebilgiacik[playerid] = 0;
				}else if(PlayerInfo[playerid][ceteid] == cetid)
				{
				    RemovePlayerMapIcon( playerid, bolgid );
					SendClientMessageEx(playerid, 0x6699FFFF,fstring);
				}else if(PlayerInfo[playerid][ceteid] == Bolge[bolgid][bolgesahip])
				{
					RemovePlayerMapIcon( playerid, bolgid );
					SendClientMessageEx(playerid, 0x6699FFFF,estring);
				}
			}
			bolgeyielegecirdik(cetid,bolgid);
		}
	}else
	{
		if(bolgeelegecirmetimer[bolgid] != -1)KillTimer(bolgeelegecirmetimer[bolgid]);
		bolgeelegecirmetimer[bolgid] = -1;
		bolgesaldirida[bolgid] = false;
		GangZoneStopFlashForAll(bolgezone[bolgid]);
		format(estring,144,"[ZONE] {25FF25}%s {FFFFFF}isimli bölgenizi {FF2525}%s {FFFFFF}çetesi ele geçiremedi.",Bolge[bolgid][bolgeismi],ceteisim[saldiran]);
