	cache = mysql_query(Owsla, Query);
	if(cache_num_rows())
	{
 		 cache_get_value_name_int(0, "bolgesayisi", bolgesayi);
  	}
  	cache_delete(cache);
  	format(Query,256,"UPDATE `ceteler` SET `bolgesayisi` = '%d' WHERE `ceteid` = '%d'",bolgesayi-1,Bolge[bolgid][bolgesahip]);
  	mysql_tquery(Owsla, Query);
    Bolge[bolgid][bolgesahip] = cetid;
    mysql_format(Owsla,Query,256,"UPDATE `bolgeler` SET  `bolgesahip` = '%d', `bolgesahipname` = '%e', `bolgerenk` = '%d' WHERE `bolgeid` = '%d'", cetid,Bolge[bolgid][bolgesahipname],Bolge[bolgid][bolgerenk],bolgid);
	mysql_tquery(Owsla,Query);
	GangZoneHideForAll(bolgezone[bolgid]);
	GangZoneShowForAll(bolgezone[bolgid], Bolge[bolgid][bolgerenk]);
	new id;
	foreach(new playerid:Player)
	{
		if(PlayerInfo[playerid][ceteid] == cetid)
		{
			id = playerid;
			break;
		}
	}
	format(estring,512,"~r~~h~%s ~w~~h~isimli cete ~r~~h~%s ~w~~h~isimli cetenin ~r~~h~%s ~w~~h~bolgesini ele gecirdi!",code(Bolge[bolgid][bolgesahipname]),code(eskiceteisim),code(Bolge[bolgid][bolgeismi]));
	TextMesaji(code(estring));
	format(Query,256,"{FF0015}%s\n{FFFFFF}Sahip: {%06x}%s\n{FFFFFF}Bölge Seviye: %d\n{93FF93}/bolge",code(Bolge[bolgid][bolgeismi]),Bolge[bolgid][bolgerenk]>>>8,code(ceteisim[id]),Bolge[bolgid][bolgeseviye]);
	UpdateDynamic3DTextLabelText(BolgeText[bolgid], 0xFFFFFFFF, Query);
}
