function MyCarPress(playerid, key_f)
{
	if(key_f == 0)return 1;
	if(GetPlayerState(playerid) != PLAYER_STATE_DRIVER) return SendClientMessageEx(playerid,0xFF0000FF,"Hata » {FFFFFF}Bir aracin soforu olmaniz lazim!");
	new Float:T[4], motor, isiklar, alarm, kapilar, kaput, bagaj, objective, vehid = GetPlayerVehicleID(playerid);
