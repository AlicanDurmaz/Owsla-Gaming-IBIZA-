stock OnlineTime(saniye)
{
	new a[32];
	if(saniye > 86400)
	{
		format(a,32,"%dd ",(saniye/86400));
		saniye%=86400;
	}
	if(saniye > 3600)
	{
		format(a,32,"%s%dh ",a,saniye/3600);
		saniye%=3600;
	}
	if(saniye > 60)
	{
		format(a,32,"%s%dm ",a,saniye/60);
		saniye%=60;
	}
	if(saniye > 0)
	{
		format(a,32,"%s%ds",a,saniye);
	}
	return a;
}
