    format(eskiceteisim,25,"%s",Bolge[bolgid][bolgesahipname]);
	format(Query,256,"SELECT * FROM `ceteler` WHERE `ceteid` = '%d'",cetid);
	new Cache:cache = mysql_query(Owsla, Query);
	if(cache_num_rows())
	{
 		cache_get_value_name_int(0, "ceterengi", Bolge[bolgid][bolgerenk]);
 		cache_get_value_name_int(0, "bolgesayisi", bolgesayi);
