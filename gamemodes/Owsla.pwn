		case 10..15: return weaponid+311;
		case 16..18: return weaponid+326;
		case 22..29: return weaponid+324;
		case 30, 31: return weaponid+325;
		case 32: return 372;
		case 33..45: return weaponid+324;
		case 46: return 371;
		case 555: return 370;
		case 666: return 1240;
		case 777: return 1242;
	}
	return 0;
}
